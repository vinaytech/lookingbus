<h1>Listing institution_app_pages</h1>

<table>
  <thead>
    <tr>
      <th>Institution app</th>
      <th>Name</th>
      <th>Url</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @institution_app_pages.each do |institution_app_page| %>
      <tr>
        <td><%= institution_app_page.institution_app_id %></td>
        <td><%= institution_app_page.name %></td>
        <td><%= institution_app_page.url %></td>
        <td><%= link_to 'Show', institution_app_page %></td>
        <td><%= link_to 'Edit', edit_institution_app_page_path(institution_app_page) %></td>
        <td><%= link_to 'Destroy', institution_app_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

